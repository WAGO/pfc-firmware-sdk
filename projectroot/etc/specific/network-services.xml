<?xml version="1.0" encoding="UTF-8"?>
<services_config>
  <dhcpd>
    <port_name>br0</port_name>
    <state>disabled</state>
    <range>192.168.1.2_192.168.1.254</range>
    <lease_time>120</lease_time>
    <ip_addr>192.168.1.1</ip_addr>
    <netmask>255.255.255.0</netmask>
  </dhcpd>
  <dhcpd>
    <port_name>br1</port_name>
    <state>disabled</state>
    <range>192.168.2.2_192.168.2.254</range>
    <lease_time>120</lease_time>
    <ip_addr>192.168.2.1</ip_addr>
    <netmask>255.255.255.0</netmask>
  </dhcpd>
  <dns>
    <state>disabled</state>
    <mode>proxy</mode>
  </dns>
</services_config>
