+       status = "okay";
+       pinctrl-names = "default", "sleep";
+       pinctrl-0 = <&gpio_bitbang_mdio_pins>;
+       pinctrl-1 = <&gpio_bitbang_mdio_sleep_pins>;
+       gpios = <&gpio0 1 0	/* 0: mdc  */
+	        &gpio0 0 0>;   	/* 1: mdio */
+
+       phy1: ethernet-phy@1 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <1>;
+       };
+
+       phy2: ethernet-phy@2 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <2>;
+       };
+};
+
+/*
+ * Use a fixed phy for emac1.
+ * that's rgmii2
+ */
+&cpsw_emac1 {
+	phy-mode = "rmii";
+	fixed-link {
+		speed = <100>;
+		full-duplex;
+	};
+};
+
+&phy_sel {
+	rmii-clock-ext;
+};
