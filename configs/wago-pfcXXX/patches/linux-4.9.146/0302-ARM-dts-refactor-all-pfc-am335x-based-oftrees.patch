+			regulator-min-microamp = <200000>;
+			regulator-max-microamp = <200000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+
+		loadswitch3:  loadswitch-ls3 {
+			compatible = "ti,tps65218-ls3";
