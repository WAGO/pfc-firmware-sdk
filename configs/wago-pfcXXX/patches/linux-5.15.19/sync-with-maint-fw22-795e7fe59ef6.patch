+/ {
+	swcfg_ksz8863: swcfg_ksz8863 {
+		compatible = "swcfg,ksz8863";
+		swcfg,mii-bus = <&bitbang_mdio0_sw>;
+		swcfg,alias = "ksz8863";
+		swcfg,cpu_port = <2>;
+		swcfg,ports = <3>;
+		swcfg,vlans = <16>;
+		swcfg,switch = <&ksz8863_switch>;
+
+		status = "disabled";
+	};
+};
+
+&bitbang_mdio0_sw {
+	ksz8863_switch: switch@0 {
+		compatible = "micrel,ksz8863";
+
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+		ksz,reset-gpio = <&io_expander_70 4 GPIO_ACTIVE_LOW>;
+
+		reg = <0>;
+		dsa,member = <0 0>;
+
+		ksz,reset-switch;
+		ksz,disable-internal-ldo;
+
+		status = "disabled";
+
+		ports {
+			#address-cells = <1>;
+			#size-cells = <0>;
+
+			port@0 {
+				reg = <2>;
+				label = "ethX1";
+			};
+
+			port@1 {
+				reg = <1>;
+				label = "ethX2";
+			};
+
+			port@2 {
+				reg = <3>;
+				label = "cpu";
+				ethernet = <&mac_sw>;
+				fixed-link {
+					speed = <100>;
+					full-duplex;
+				};
+			};
+		};
+	};
+};
