+		.name = "rate_limit",
+		.description =
+			"Enables rate limiting on egress"
+			" Port3(CPU-Ingress), format='50.mbps'/'64.kbps'. 'off'"
+			" will disable rate limiting.",
+		.set = ksz8863_cfg_set_ratelimit,
+		.get = ksz8863_cfg_get_ratelimit,
