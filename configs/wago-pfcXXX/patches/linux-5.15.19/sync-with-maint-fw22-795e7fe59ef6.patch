+static inline int
+ksz8863_cfg_get_static_mac_entry(struct ksz8863_cfg *cfg, u8 idx,
+				 struct ksz8863_static_mac_tbl_entry *entry)
+{
+	return cfg->chip->mii_ops->read_table(cfg->chip, KSZ8863_TBL_STATIC_MAC,
+					      idx, (u8 *)entry, sizeof(*entry));
+}
