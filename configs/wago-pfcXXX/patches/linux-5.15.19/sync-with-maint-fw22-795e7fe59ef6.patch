+static void serial_omap_uart_qos_work(struct work_struct *work)
+{
+	struct uart_omap_port *up = container_of(work, struct uart_omap_port,
+						qos_work);
+
+	cpu_latency_qos_update_request(&up->pm_qos_request, up->latency);
+}
