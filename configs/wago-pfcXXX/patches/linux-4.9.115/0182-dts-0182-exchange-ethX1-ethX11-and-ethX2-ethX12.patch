From 070e6a2667b65b46ca88bc7639d8994557b027d2 Mon Sep 17 00:00:00 2001
From: Andreas Schmidt <andreas.schmidt@wago.com>
Date: Wed, 20 Jun 2018 09:22:06 +0200
Subject: [PATCH 182/261] dts: 0182: exchange ethX1 -> ethX11 and ethX2 ->
 ethX12

Signed-off-by: Andreas Schmidt <andreas.schmidt@wago.com>
---
 arch/arm/boot/dts/wago-pfc200v3-devconf-0182.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/wago-pfc200v3-devconf-0182.dts b/arch/arm/boot/dts/wago-pfc200v3-devconf-0182.dts
index 6d9abda..8a02dee 100644
--- a/arch/arm/boot/dts/wago-pfc200v3-devconf-0182.dts
+++ b/arch/arm/boot/dts/wago-pfc200v3-devconf-0182.dts
@@ -148,14 +148,14 @@
 
                         port@0 {
                                 reg = <0>;
-                                label = "ethX11";
+                                label = "ethX1";
                                 phy-handle = <&ext_phy0>;
                                 phy-mode = "sgmii";
                         };
 
                         port@1 {
                                 reg = <1>;
-                                label = "ethX12";
+                                label = "ethX2";
                                 phy-handle = <&ext_phy1>;
                                 phy-mode = "sgmii";
                         };
@@ -172,14 +172,14 @@
 
                         port@3 {
                                 reg = <3>;
-                                label = "ethX1";
+                                label = "ethX11";
                                 phy-handle = <&int_phy3>;
                                 phy-mode = "rmii";
                         };
 
                         port@4 {
                                 reg = <4>;
-                                label = "ethX2";
+                                label = "ethX12";
                                 phy-handle = <&int_phy4>;
                                 phy-mode = "rmii";
                         };
-- 
