+		
+		/*  used for (XE164 KBUS communication) */
+		0x150 (PIN_INPUT_PULLUP		| SLEWCTRL_SLOW	| MUX_MODE0)	/* spi0_sclk.spio_sclk */ 		//a17	CLK-XE164
