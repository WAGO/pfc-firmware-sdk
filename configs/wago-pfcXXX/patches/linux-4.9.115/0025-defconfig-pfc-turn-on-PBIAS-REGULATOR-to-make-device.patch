@@ -349,6 +349,7 @@ CONFIG_OMAP_WATCHDOG=y
 CONFIG_MFD_TI_AM335X_TSCADC=y
 CONFIG_MFD_TPS65218=y
 CONFIG_MFD_TPS65910=y
