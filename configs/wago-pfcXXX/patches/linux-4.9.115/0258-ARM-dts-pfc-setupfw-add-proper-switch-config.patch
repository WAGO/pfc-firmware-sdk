 };
 
+&swcfg_ksz8863 {
+	swcfg,alias = "ksz8863";
+};
+
 &uart1 {
 	/* uart1 disabled for cmtest gpio test */
 	status = "disabled";
diff --git a/arch/arm/boot/dts/am3505-pfc200-setupfw.dts b/arch/arm/boot/dts/am3505-pfc200-setupfw.dts
index ac2c61c..121a2ff 100644
