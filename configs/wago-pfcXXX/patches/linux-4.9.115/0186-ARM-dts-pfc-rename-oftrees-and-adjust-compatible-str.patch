+
+	cpsw_sleep_pins: pinmux_cpsw_sleep_pins {
+		pinctrl-single,pins = <
+			/* RMMI2 */
+			0x040 (PIN_OUTPUT | SLEWCTRL_FAST | MUX_MODE3) /* gpmc_a0.rmii2_txen */		//r13
+			0x050 (PIN_OUTPUT | SLEWCTRL_FAST | MUX_MODE3) /* gpmc_a4.rmii2_txd1 */		//r14
+			0x054 (PIN_OUTPUT | SLEWCTRL_FAST | MUX_MODE3) /* gpmc_a5.rmii2_txd0 */		//v15
+			0x068 (PIN_INPUT		  | MUX_MODE3) /* gpmc_a10.rmii2_rxd1 */	//t16
+			0x06C (PIN_INPUT		  | MUX_MODE3) /* gpmc_a11.rmii2_rxd0 */	//v17
+			0x108 (PIN_INPUT		  | MUX_MODE1) /* mii1_col.rmii2_refclk */	//h16
+			0x088 (PIN_INPUT		  | MUX_MODE2) /* gpmc_csn3.rmii2_crs_dv */	//t13
+
+			/* RMMI1 only used on Marvell based devices; default off */
+			0x144 (PIN_INPUT           	  | MUX_MODE7) /* rmii1_ref_clk.rmii1_refclk      */	//h18
+			0x114 (PIN_INPUT	 	  | MUX_MODE7) /* mii1_txen.rmii1_txen (gpio3_3)  */	//j16
+			0x128 (PIN_INPUT	 	  | MUX_MODE7) /* mii1_txd0.rmii1_txd0 (gpio0_28) */	//k17
+			0x124 (PIN_INPUT	 	  | MUX_MODE7) /* mii1_txd1.rmii1_txd1 (gpio0_21) */	//k16
+			0x140 (PIN_INPUT	  	  | MUX_MODE7) /* mii1_rxd0.rmii1_rxd0 (gpio2_21) */	//m16
+			0x13c (PIN_INPUT	  	  | MUX_MODE7) /* mii1_rxd1.rmii1_rxd1 (gpio2_20) */	//l15
+			0x10c (PIN_INPUT	  	  | MUX_MODE7) /* mii1_crs.rmii1_crs_dv (gpio3_1) */	//h17
+		>;
+	};
+
+	gpio_bitbang_mdio_pins: pinmux_gpio_bitbang_mdio_pins {
+		pinctrl-single,pins = <
+			/* MDIO */
+			0x148 (PIN_INPUT_PULLUP | SLEWCTRL_FAST | MUX_MODE7)	/* M17: mdio.gpio0_0 (mdio_data), SLEWCTRL_FAST?? */
+			0x14c (PIN_OUTPUT_PULLUP | MUX_MODE7)			/* M18:  mdc.gpio0_1 (mdio_clk)                   */
+		>;
+	};
+
+	gpio_bitbang_mdio_sleep_pins: pinmux_gpio_bitbang_mdio_sleep_pins {
+		pinctrl-single,pins = <
+			/* MDIO reset value */
+			0x148 (PIN_INPUT_PULLUP 	| MUX_MODE7)	/* mdio.gpio0_0 */				//m17
+			0x14c (PIN_INPUT_PULLDOWN 	| MUX_MODE7)	/* mdc.gpio0_1   */				//m18
+		>;
+	};
+
+	kbus_pins: pinmux_kbus_pins {
+		pinctrl-single,pins = <
+			/* used for (XE164 KBUS io control) */
+			0x058 (PIN_INPUT_PULLUP  		 | MUX_MODE7)   /* gpmc_a6.gpio1_22 */		//u15	NSYNC-XE164
+			0x05C (PIN_INPUT_PULLUP	 		 | MUX_MODE7)   /* gpmc_a7.gpio1_23 */		//t15	NIRQ-XE164
+			0x1b4 (PIN_INPUT_PULLUP	 		 | MUX_MODE7)   /* xdma_event_intr1.gpio0_20 */	//d14	nRDY-XE164
+			0x1e4 (PIN_OUTPUT_PULLUP | SLEWCTRL_SLOW | MUX_MODE7)	/* emu0.gpio3_7 */		//c14	CMDSEL-XE164
+			/* Error Pin */
+			0x060 (PIN_INPUT_PULLUP			 | MUX_MODE7)	/* gpmc_a8.gpio1_24 */		//v16	NERR-XE164
+		>;
+	};
+
+	gpio0_pins: pinmux_gpio0_pins {
+		pinctrl-single,pins = <
+			/*  used for (io control) */
+			0x074 (PIN_INPUT_PULLUP	| MUX_MODE7) /* gpmc_wpn.gpio0_31 */	//u17	nINT-PHY2 Lantiq Phy 2(old RMII2.RXER)
+			/* timer 4 as watchdog ip*/
+			0x1b0 (PIN_OUTPUT_PULLUP	| SLEWCTRL_SLOW	| MUX_MODE7)	/* xdma_event_intr0.gpio0_19 */	//a15	RESET-LED_Server
+		>;
+	};
+
+	gpio1_pins: pinmux_gpio1_pins {
+		pinctrl-single,pins = <
+			/*  used for (io control) */
+			0x044 (PIN_INPUT_PULLUP	 		 | MUX_MODE7) /* gpmc_a1.gpio1_17 */ //v14 ALARM-USV (old RMII1.nINT)
+			0x048 (PIN_OUTPUT_PULLUP | SLEWCTRL_SLOW | MUX_MODE7) /* gpmc_a2.gpio1_18 */ //u14 BOOT-SEL
+		>;
+	};
+
+	gpio3_pins: pinmux_gpio3_pins {
+		pinctrl-single,pins = <
+			/*  used for (io control) */
+			0x110 (PIN_INPUT_PULLUP | MUX_MODE7) /* mii1_rxerr.gpio3_2 	*/	//j15 nINT-PHY1 Lantiq Phy 2(old RMII1.RXER)
+			0x1A8 (PIN_INPUT_PULLUP	| MUX_MODE7) /* mcasp0_axr1.gpio3_20 	*/	//d13 nINT-PMIC
+			0x1Ac (PIN_INPUT_PULLUP	| MUX_MODE7) /* mcasp0_ahclkx.gpio3_21	*/	//a14 nINT-RTC
+			/*  used for (BAS-SPS-Program-Switch ) */
+			0x198 (PIN_INPUT	| MUX_MODE7) /* mcasp0_axr0.gpio3_16  	*/	//d12 nRST-BAS
+			0x19c (PIN_INPUT	| MUX_MODE7) /* mcasp0_ahclkr.gpio3_17	*/	//c12 nSTOP-BAS
+			0x1A0 (PIN_INPUT	| MUX_MODE7) /* mcasp0_aclkr.gpio3_18 	*/	//b12 nRUN-BAS
+			/*  used for (RAB-Reset Button ) */
+			0x194 (PIN_INPUT	| MUX_MODE7) /* mcasp0_fsx.gpio3_15   	*/	//b13 RESET-ALL-BUTTON
+		>;
+	};	
+
+	usb0_pins: pinmux_usb0_pins {
+		pinctrl-single,pins = <
+			/*  used for (USB) */
+			0x21c (PIN_OUTPUT_PULLDOWN | SLEWCTRL_SLOW | MUX_MODE0)	/* USB0_DRVVBUS.USB0_DRVVBUS */	//f16 DRIVE-Enable
+		>;
+	};
+
+	usb1_pins: pinmux_usb1_pins {
+		pinctrl-single,pins = <
+			/*  used for (USB) */
+			0x234 (PIN_OUTPUT_PULLDOWN | SLEWCTRL_SLOW | MUX_MODE0)	/* USB1_DRVVBUS.USB1_DRVVBUS */	//f15 DRIVE-Enable
+		>;
+	};
+};
+
+&gpmc {
+	pinctrl-names = "default";
+	pinctrl-0 = <&gpmc_pins>;
+
+	num-cs = <7>;	/* only two devices */
+	num-waitpins = <3>;
+	status = "okay";
+	ranges = <
+		4 0 0x01000000 0x01000000	/* CS4: NVRAM, 16M */
+		0 0 0x04000000 0x01000000	/* CS0: NETX/DPC31, 16M */
+	>;
+
+	nvram: nor@4,0 { /* NVRAM Device 128k */
+		reg = <4 0 0x01000000>;
+		bank-width = <2>; /* 1: 8bit, 2: 16bit */
+		gpmc,sync-clk-ps = <0>;
+		gpmc,cs-on-ns = <0>;
+		gpmc,cs-rd-off-ns = <50>;
+		gpmc,cs-wr-off-ns = <30>;
+		gpmc,adv-on-ns = <0>;
+		gpmc,adv-rd-off-ns = <0>;
+		gpmc,adv-wr-off-ns = <0>;
+		gpmc,oe-on-ns = <0>;
+		gpmc,oe-off-ns = <50>;
+		gpmc,we-on-ns = <0>;
+		gpmc,we-off-ns = <30>;
+		gpmc,rd-cycle-ns = <50>;
+		gpmc,wr-cycle-ns = <30>;
+		gpmc,access-ns = <40>;
+		gpmc,wr-access-ns = <10>;
+		gpmc,wr-data-mux-bus-ns = <0>;
+		gpmc,cycle2cycle-delay-ns = <10>;
+		gpmc,cycle2cycle-samecsen;
+		gpmc,cycle2cycle-diffcsen;
+
+		#address-cells = <1>;
+		#size-cells = <1>;
+	};
+};
+
+&uart0 { /* debug interface over JTAG Connector an Service Port */
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart0_pins>;
+	status = "okay";
+};
+
+&uart1 {
+	status = "disabled";
+};
+
+&uart5 { /* KBUS Firmware download */
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart5_pins>;
+	status = "okay";
+};
+
+&spi0 {
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&spi0_pins>;
+	spi-rt;
+	spi-rt-prio = <81>;
+
+	kbus0: kbus@0 {	/* KBUS on cs0 */
+		compatible = "wago,spi-kbus";
+		status = "okay";
+		pinctrl-names = "default";
+		pinctrl-0 = <&kbus_pins>;
+		reg = <0>;
+		spi-max-frequency = <10000000>; /* FIXME */
+		kbus,dma-boost;
+		kbus,dma-boost-prio   = <85>;
+		kbus,dma-boost-irq-thread = "irq/20-49000000";
+		kbus,dma-default-prio = <50>;
+
+		/*
+		 * announce used tty-device in /sys/class/wago/system.
+		 * is needed to link this device to /dev/ttyKbus
+		 */
+		kbus,tty-device = "ttyO5"; /* corresponds to &uart5*/
+
+		/* some should be active low, keep all to high for compatibility reasons */
+		kbus,gpio-nrst   = <&io_expander_70 3 GPIO_ACTIVE_HIGH>;
+		kbus,gpio-nsync  = <&gpio1 22 GPIO_ACTIVE_HIGH>;
+		kbus,gpio-cmdsel = <&gpio3 7 GPIO_ACTIVE_HIGH>;
+		kbus,gpio-nirq   = <&gpio1 23 GPIO_ACTIVE_HIGH>;
+		kbus,gpio-nerr   = <&gpio1 24 GPIO_ACTIVE_HIGH>;
+		kbus,gpio-nrdy   = <&gpio0 20 GPIO_ACTIVE_HIGH>;
+	};
+};
+
+&i2c0 {
+	status = "okay";
+	clock-frequency = <100000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&i2c0_pins>;
+
+	io_expander_70: pca9554@70 { /* IO Port Expander only outputs*/
+		compatible = "nxp,pca9554";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		#gpio-cells = <2>;
+		gpio-controller;
+		reg = <0x70>;
+	};
+
+	led_bar1: pca9552@60 { /* System status Visualization (LED Port Expander "SYS / RUN / IO / MS / NS / CAN-lED")*/
+		compatible = "nxp,pca9552";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		reg = <0x60>;
+
+		sys_r: sys-red@0 {
+		      label = "sys-red";
+		      reg = <0>;
+		      linux,default-trigger = "timer";
+		};
+
+		sys_g: sys-green@1 {
+		      label = "sys-green";
+		      reg = <1>;
+		      linux,default-trigger = "timer";
+		};
+
+		run_r: run-red@2 {
+		      label = "run-red";
+		      reg = <2>;
+		      linux,default-trigger = "none";
+		};
+
+		run_g: run-green@3 {
+		      label = "run-green";
+		      reg = <3>;
+		      linux,default-trigger = "none";
+		};
+
+		io_r: io-red@4 {
+		      label = "io-red";
+		      reg = <4>;
+		      linux,default-trigger = "none";
+		};
+
+		io_g: io-green@5 {
+		      label = "io-green";
+		      reg = <5>;
+		      linux,default-trigger = "none";
+		};
+
+		ms_r: ms-red@6 {
+		      label = "ms-red";
+		      reg = <6>;
+		      linux,default-trigger = "none";
+		};
+
+		ms_g: ms-green@7 {
+		      label = "ms-green";
+		      reg = <7>;
+		      linux,default-trigger = "none";
+		};
+
+		ns_r: ns-red@8 {
+		      label = "ns-red";
+		      reg = <8>;
+		      linux,default-trigger = "none";
+		};
+
+		ns_g: ns-green@9 {
+		      label = "ns-green";
+		      reg = <9>;
+		      linux,default-trigger = "none";
+		};
+
+		u7_r: u7-red@10 {
+		      label = "u7-red";
+		      reg = <10>;
+		      linux,default-trigger = "none";
+		};
+
+		u7_g: u7-green@11 {
+		      label = "u7-green";
+		      reg = <11>;
+		      linux,default-trigger = "none";
+		};
+
+		/* this will switch of the sys-red led during startup */
+		sys-over-red@15 {
+		      label = "sys-over-red";
+		      reg = <15>;
+		      linux,default-trigger = "default-on";
+		};
+	};
+
+	led_bar0: pca9552@61 { /* System status Visualization (LED Port Expander "BF / DIA / U4 / U3 / U2 / U1-lED")*/
+		compatible = "nxp,pca9552";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		reg = <0x61>;
+
+		u6_r: u6-red@0 {
+		      label = "u6-red";
+		      reg = <0>;
+		      linux,default-trigger = "none";
+		};
+
+		u6_g: u6-green@1 {
+		      label = "u6-green";
+		      reg = <1>;
+		      linux,default-trigger = "none";
+		};
+
+		u5_r: u5-red@2 {
+		      label = "u5-red";
+		      reg = <2>;
+		      linux,default-trigger = "none";
+		};
+
+		u5_g: u5-green@3 {
+		      label = "u5-green";
+		      reg = <3>;
+		      linux,default-trigger = "none";
+		};
+
+		u4_r: u4-red@4 {
+		      label = "u4-red";
+		      reg = <4>;
+		      linux,default-trigger = "none";
+		};
+
+		u4_g: u4-green@5 {
+		      label = "u4-green";
+		      reg = <5>;
+		      linux,default-trigger = "none";
+		};
+
+		u3_r: u3-red@6 {
+		      label = "u3-red";
+		      reg = <6>;
+		      linux,default-trigger = "none";
+		};
+
+		u3_g: u3-green@7 {
+		      label = "u3-green";
+		      reg = <7>;
+		      linux,default-trigger = "none";
+		};
+
+		u2_r: u2-red@8 {
+		      label = "u2-red";
+		      reg = <8>;
+		      linux,default-trigger = "none";
+		};
+
+		u2_g: u2-green@9 {
+		      label = "u2-green";
+		      reg = <9>;
+		      linux,default-trigger = "none";
+		};
+
+		u1_r: u1-red@10 {
+		      label = "u1-red";
+		      reg = <10>;
+		      linux,default-trigger = "none";
+		};
+
+		u1_g: u1-green@11 {
+		      label = "u1-green";
+		      reg = <11>;
+		      linux,default-trigger = "none";
+		};
+	};
+
+	at24cs128n { /* EEPROM identification*/
+		compatible = "atmel,24c512";
+		reg = <0x54>;
+		pagesize = <128>;
+	};
+
+	rtc_r2221t { /* Real Time clock IC  */
+		compatible = "ricoh,r2221tl";
+		reg = <0x32>;
+		interrupt-parent = <&gpio3>;
+		interrupts = <21 IRQ_TYPE_LEVEL_LOW>;
+		trim-data = <0 1 31>;
+	};
+
+	tps65218@24 {	/* Power Managment IC  */
+		compatible = "ti,tps65218";
+		reg = <0x24>;
+		interrupt-parent = <&gpio3>;
+		interrupts = <20 IRQ_TYPE_LEVEL_LOW>;
+		interrupt-controller;
+		#interrupt-cells = <2>;
+
+		dcdc1_reg: regulator-dcdc1 {
+			compatible = "ti,tps65218-dcdc1";
+			/* VDD_CORE +1V1-CORE voltage limits */
+			regulator-name = "vdd_CORE";
+			regulator-min-microvolt = <1100000>;
+			regulator-max-microvolt = <1100000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+	
+		dcdc2_reg: regulator-dcdc2 {
+			compatible = "ti,tps65218-dcdc2";
+			/* VDD_MPU +1V1-MPU voltage limits */
+			regulator-name = "vdd_MPU";
+			regulator-min-microvolt = <912000>;
+			regulator-max-microvolt = <1378000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+	
+		dcdc3_reg: regulator-dcdc3 {
+			compatible = "ti,tps65218-dcdc3";
+			/* VDD_DDR +1V5-DDR voltage limits */
+			regulator-name = "vdd_DDR";
+			regulator-min-microvolt = <1500000>;
+			regulator-max-microvolt = <1500000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+	
+		dcdc4_reg: regulator-dcdc4 {
+			compatible = "ti,tps65218-dcdc4";
+			/* VDD_LOGIC +3V3 voltage limits */
+			regulator-name = "vdd_LOGIC";
+			regulator-min-microvolt = <3300000>;
+			regulator-max-microvolt = <3300000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+	
+		loadswitch2:  loadswitch-ls2 {
+			compatible = "ti,tps65218-ls2";
+			/* VDD_LS2 +5V15 for USB */
+			regulator-name = "vdd_LS2_USB";
+			/* Should be set to 100 mA according to SLP00000843.002
+			 * but 4G Modem is not being inumurated correctly,
+			 * so we increase it.
+			 */
+			regulator-min-microamp = <200000>;
+			regulator-max-microamp = <200000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+
+		loadswitch3:  loadswitch-ls3 {
+			compatible = "ti,tps65218-ls3";
+			/* VDD_LS3 +5V15 for FPGA and Ethernet */
+			regulator-name = "vdd_LS3_FPGA_ETHERNET";
+			regulator-min-microamp = <1000000>;
+			regulator-max-microamp = <1000000>;
+			regulator-boot-on;
+			regulator-always-on;
+		};
+
+		ldo1: regulator-ldo1 {	//VDD-PLL  +1V8
+			compatible = "ti,tps65218-ldo1";
+			regulator-name = "ldo1";
+			regulator-always-on;
+		};
+	};
+};
+
+&mac {
+	pinctrl-names = "default";
+	pinctrl-0 = <&cpsw_default_pins>;
+	pinctrl-1 = <&cpsw_sleep_pins>;
+	mac_control = <0x18021>; /* force 100MBit full-duplex */
+	active_slave = <1>;
+	status = "okay";
+};
+
+&bitbang_mdio0 {
+       status = "okay";
+       pinctrl-names = "default", "sleep";
+       pinctrl-0 = <&gpio_bitbang_mdio_pins>;
+       pinctrl-1 = <&gpio_bitbang_mdio_sleep_pins>;
+       gpios = <&gpio0 1 0	/* 0: mdc  */
+		&gpio0 0 0>;   	/* 1: mdio */
+
+       phy1: ethernet-phy@1 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <1>;
+       };
+
+       phy2: ethernet-phy@2 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <2>;
+       };
+};
+
+
+/*
+ * Use a fixed phy for emac1.
+ * that's rgmii2
+ */
+&cpsw_emac1 {
+	phy-mode = "rmii";
+	fixed-link {
+		speed = <100>;
+		full-duplex;
+	};
+};
+
+&phy_sel {
+	rmii-clock-ext;
+};
+
+&mmc1 {
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&sdcard_pins>;
+	vmmc-supply = <&dcdc4_reg>;
+	bus-width = <4>;
+	cd-gpios = <&gpio1 19 GPIO_ACTIVE_LOW>;
+};
+
+&mmc2 {
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&emmc_pins>;
+	vmmc-supply = <&dcdc4_reg>;
+	bus-width = <4>;
+	ti,non-removable;
+};
+
+/*??????*/
+&sham {
+	status = "okay";
+};
+
+&aes { status = "okay"; };
+
+&elm {
+	status = "okay";
+};
+
+&gpio1 {
+	boot_select {
+		gpio-hog;
+		gpios = <18 GPIO_ACTIVE_LOW>;
+		output-high;
+		line-name = "boot_select";
+	};
+};
+
