+
+&am33xx_pinmux {
+	usb_otg_pins: pinmux_usb_otg_pins {
+		pinctrl-single,pins = <
+			/* USB OTG Mux Config */
+			0x234 (PIN_OUTPUT_PULLUP | MUX_MODE0)  /* usb1_drvvbus.usb1_drvvbus */
+		>;
+	};
+};
+
+&usb {
+	status = "okay";
+};
+
+&usb_ctrl_mod {
+	status = "okay";
+};
+
+&usb0_phy {
+	status = "okay";
+};
+
+&usb0 {
+	status = "okay";
+	dr_mode = "peripheral";
+};
+
+&usb1_phy {
+	status = "okay";
+	/* status = "disabled"; */
+};
+
+/* usb1 - host */
+&usb1 {
+	status = "okay";
+	dr_mode       = "host";
+	pinctrl-names = "default";
+	pinctrl-0     = <&usb_otg_pins>;
+
+	multipoint = <1>;
+	num-eps    = <16>;
+	ram-bits   = <12>;
+
+	interface-type = <0>;	/* ULPI */
+	mode           = <1>;	/* HOST Mode */
+	power          = <250>;	/* can supply 250mA when operating in host mode */
+};
diff --git a/arch/arm/boot/dts/am335x-pfc-750_810x-uart1.dtsi b/arch/arm/boot/dts/am335x-pfc-750_810x-uart1.dtsi
