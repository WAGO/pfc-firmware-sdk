+		mdio-gpio0 = &bitbang_mdio0;
+	};
+
+	memory {
+		device_type = "memory";
+		reg = <0x80000000 0x20000000>; /* 512 MB */
+	};
+
+	fixedregulator@0 { /* XE164 +3V3  System voltage */
+		compatible = "regulator-fixed";
+		regulator-name = "vxe164";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		vin-supply = <&dcdc4_reg>;
+	};
+
+	fixedregulator@1 { /* SD Card +3V3 Systemvoltage over power switch */
+		compatible = "regulator-fixed";
+		regulator-name = "v_sd";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+                vin-supply = <&dcdc4_reg>;
+	};
+
+	UIO_NVRAM@0x1000000 {
+		compatible = "uio_pdrv_genirq";
+		reg = <0x01000000 0x00020000>;  /* 128k 16bit NVRAM */
+	};
+
+	wsysinit: wsysinit_init {
+		compatible = "wago,sysinit";
+		add-sysfs-entries;
+	
+		tty,service   = "ttyO0";
+		tty,rs232-485 = "ttyO1";
+
+		board,variant = "pfc200v3";
+	
+		/* sysclock adjustments, empirical values */
+		adjtimex,tick = <10000>;
+		adjtimex,frequency = <200000>;
+	};
+
+	/* this name of the gpio-keys device is a
+	 * historical heritage from 3.6.11 kernel.
+	 * the device-name is checked in omsd. So we
+	 * need to adapt this configuration
+	 */
+	oms: PAC-Operating-Mode-Switch {
+		compatible = "gpio-keys";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		autorepeat;
+	
+		run {
+			label = "RUN";
+			gpios = <&gpio3 18 GPIO_ACTIVE_LOW>; /* BAS_RUN GPIO 114 */
+			linux,code = <1>;
+			linux,input-type = <5>;  /* EV_SW */
+			debounce-interval = <1>; /* debounce ticks interval in msecs */
+		};
+	
+		stop {
+			label = "STOP";
+			gpios = <&gpio3 17 GPIO_ACTIVE_LOW>; /* BAS_RUN GPIO 113 */
+			linux,code = <2>;
+			linux,input-type = <5>;  /* EV_SW */
+			debounce-interval = <1>; /* debounce ticks interval in msecs */
+		};
+	
+		reset {
+			label = "RESET";
+			gpios = <&gpio3 16 GPIO_ACTIVE_LOW>; /* BAS_RUN GPIO 112 */
+			linux,code = <3>;
+			linux,input-type = <1>;  /* EV_KEY */
+			debounce-interval = <1>; /* debounce ticks interval in msecs */
+		};
+	
+		reset_all {
+			label = "RESET_ALL";
+			gpios = <&gpio3 15 GPIO_ACTIVE_HIGH>; /* BAS_RUN GPIO 111 */
+			linux,code = <4>;
+			linux,input-type = <1>;  /* EV_KEY */
+			debounce-interval = <1>; /* debounce ticks interval in msecs */
+		};
+	};
+
+	watchdog: watchdog {
+		/* XC6124 */
+		compatible = "linux,wdt-gpio";
+		pinctrl-names = "default";
+		pinctrl-0 = <&watchdog_pins>;
+
+		gpio-en = <&io_expander_70 0 GPIO_ACTIVE_LOW>;
+		gpios = <&gpio3 8 GPIO_ACTIVE_LOW>;
+
+		hw_algo = "toggle";
+		hw_margin_ms = <1600>;
+		status = "okay";
+
+		rt-prio = <91>;
+	};
+};
+
+&ksz8863 {
+	ksz,reset-gpio = <&io_expander_70 4 GPIO_ACTIVE_LOW>;
+	ksz,disable-internal-ldo;
+};
+
+&dsa {
+	switch@0 {
+		port@0 {
+			reg = <2>;        /* phys are twisted on hw */
+			phy-handle = <&phy2>;
+		};
+
+		port@1 {
+			reg = <1>;
+			phy-handle = <&phy1>;
+		};
+	};
+};
