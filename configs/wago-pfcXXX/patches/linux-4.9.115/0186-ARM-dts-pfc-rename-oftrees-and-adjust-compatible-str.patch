+/ {
+	target_info {
+		/*
+		 * maybe we should change the drivers name
+		 * to something like "wago,information" ??
+		*/
+		compatible = "wago,testenv";
+
+		board_info {
+			model = "pfc100";
+			id = "750-81xx";
+		};
+
+		led_info {
+			/* conforms to led order on device, see node "pca9552@60".
+			 * we assume, that every led has to colors (red, green).
+			 * so we only allow the colors red,green,orange
+			 */
+			led_array_red = "sys:run:io:ms:ns:usr";
+		};
+
+		commission_pin {
+			/* this gpio pin indicates in commission mode that
+			 * the boot procedure is finished and the commission-test
+			 * could be started
+			 */
+                       pin_number   = "104"; /* emu1.gpio3_8 */
+                       signal_value = "0";
+               };
+	};
+};
+
+&am33xx_pinmux {
+
+	pinctrl-names = "default";
+	pinctrl-0 = <
+		&emu_pins
+	>;
+
+	emu_pins: pinmux_emu_pins {
