+			0x130 (PIN_OUTPUT_PULLDOWN 	| MUX_MODE7)	/* mii1_rx_clk.gpio3_10  (gpio3_10), rmii2.en/Reset    */   //l18
+			0x160 (PIN_INPUT_PULLDOWN       | MUX_MODE7)    /* spi0_cs1.gpio0_6       (gpio0_6), rmii2.nint/Int    */   //c15
