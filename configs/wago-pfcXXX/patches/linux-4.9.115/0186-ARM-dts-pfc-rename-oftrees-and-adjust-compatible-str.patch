+/dts-v1/;
+
+#include "am335x-pfc-750_810x.dtsi"
+#include "am335x-pfc-750_810x-uart1.dtsi"
+#include "am335x-pfc-750_810x-musb.dtsi"
+#include "am335x-pfc-750_810x-cmtests.dtsi"
