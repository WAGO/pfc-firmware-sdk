+++ b/arch/arm/boot/dts/wago-pfc200-dpc31.dtsi
@@ -103,8 +103,8 @@
 		gpmc,wait-monitoring-ns = <0>;
 
 		gpmc,wait-pin = <1>;
-		gpmc,wait-on-read = "true";
-		gpmc,wait-on-write = "true";
+		gpmc,wait-on-read;
+		gpmc,wait-on-write;
 
 		#address-cells = <1>;
 		#size-cells = <1>;
