+++ b/arch/arm/boot/dts/wago-src.dtsi
@@ -237,8 +237,8 @@
 		gpmc,rd-cycle-ns = <82>;
 		gpmc,wr-cycle-ns = <82>;
 		gpmc,wait-pin = <0>;
-		gpmc,wait-on-read = "true";
