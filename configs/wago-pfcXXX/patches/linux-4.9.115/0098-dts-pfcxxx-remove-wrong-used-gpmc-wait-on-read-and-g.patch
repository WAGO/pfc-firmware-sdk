 		gpmc,wait-pin = <0>;			/* WAIT input pin is WAIT0 */
-		gpmc,wait-on-read = "false";		/* =true */
-		gpmc,wait-on-write = "false";
 
 		#address-cells = <1>;
 		#size-cells = <1>;
