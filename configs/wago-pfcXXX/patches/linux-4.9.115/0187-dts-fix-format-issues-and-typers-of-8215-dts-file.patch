-                };
-        };
+	ext_phy0: ethernet-phy@0 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <0>;
+	};
+
+	ext_phy1: ethernet-phy@1 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <1>;
+	};
+
+	int_phy3: ethernet-phy@3 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <3>;
+	};
+
+	int_phy4: ethernet-phy@4 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <4>;
+	};
+
+	switch: switch@0 {
+		compatible = "marvell,mv88e6085";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		reg = <0>;
+		dsa,member = <0 0>;
+		reset-gpios = <&io_expander_70 4 GPIO_ACTIVE_LOW>;
+		phy-scan;
+
+		ports {
+			#address-cells = <1>;
+			#size-cells = <0>;
+
+			port@0 {
+				reg = <0>;
+				label = "ethX1";
+				phy-handle = <&ext_phy0>;
+				phy-mode = "sgmii";
+			};
+
+			port@1 {
+				reg = <1>;
+				label = "ethX2";
+				phy-handle = <&ext_phy1>;
+				phy-mode = "sgmii";
+			};
+
